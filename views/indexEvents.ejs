<%- include('partials/header'); -%>

<div class="container">
  <h2><%= myPageTitle %></h2>
</div>

<div class="row container">
  <div class="four columns"><a href="/events/new">Create new event</a></div>
</div>

<div class="container">
    <table class="u-full-width">
        <thead>
          <tr>
            <th>Date</th>
            <th>Event Title</th>
            <th>Description</th>
            <th> </th>
          </tr>
        </thead>

        <tbody>

          <% events.forEach(e => { %>
            <tr>
              <td><%= events.date %></td>
              <td><%= events.title %></td>
              <td><%= events.description %></td>
              <td><button>Dlt</button>
              </td>
          </tr>
        <% }) %>

        </tbody>
      </table>
</div>

<%- include('partials/footer'); -%>